{
    "name": "yiisoft/db-mssql",
    "description": "MSSQL Server extension for the Yii framework",
    "keywords": [
        "yii",
        "mssql",
        "sql server"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/db-mssql/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/db-mssql"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com",
            "homepage": "http://www.yiiframework.com/"
        },
        {
            "name": "Timur Ruziev",
            "email": "resurtm@gmail.com",
            "homepage": "http://resurtm.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/arrays": "^3.0@dev",
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.0",
        "yiisoft/di": "dev-master",
        "yiisoft/log": "dev-master",
        "yiisoft/cache": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Db\\Mssql\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Db\\Mssql\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}