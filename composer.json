{
    "name": "yiisoft/db-mssql",
    "description": "MSSQL Server extension for the Yii framework",
    "keywords": ["yii", "mssql", "sql server"],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/db-mssql/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/db-mssql"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com",
            "homepage": "http://www.yiiframework.com/"
        },
        {
            "name": "Timur Ruziev",
            "email": "resurtm@gmail.com",
            "homepage": "http://resurtm.com/"
        }
    ],
    "require": {
        "yiisoft/core": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "yiisoft/di": "dev-master",
        "yiisoft/log": "dev-master",
        "yiisoft/cache": "dev-master"
    },
    "autoload": {
        "psr-4": {"yii\\mssql\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"yiiunit\\mssql\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
