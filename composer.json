{
    "name": "yiisoft/db-mssql",
    "description": "MSSQL Server extension for the Yii framework",
    "keywords": [
        "yii",
        "mssql",
        "sql server"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/db-mssql/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/db-mssql"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ext-pdo": "*",
        "php": "^7.4|^8.0",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/db": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.6",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2",
        "yiisoft/aliases": "^1.1",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Db\\Mssql\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Db\\Mssql\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": [
            "phpunit --colors --no-interaction"
        ]
    }
}
